<project name="project" default="build">
	<target name="build">
		<echo message="Building the Message Broker Project C:\AutoDeployment\BARfiles\HttpApplication"/>
		<exec executable="/opt/IBM/ace-11.0.0.4/tools/mqsicreatebar.exe" spawn="false">
			<arg value="-data"/>
			<arg value="/root/IBM/ACET11/workspace"/>
			<arg value="-b"/>
			<arg value="/root/IBM/ACET11/workspace/BARfiles/HttpApplication.bar"/>
			<!-- List all the message flow projects -->
			<arg value="-a"/>
			<arg value="HttpApplication"/>
		</exec>
	</target>
</project>
